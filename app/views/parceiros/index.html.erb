<div id="parceiroadmin">
  <h2><b>Consultar e Relatório</b> parceiro</h2><br/>
  
  <div id='consulta'> 
   <%= form_tag consultar_parceiros_path, method: :get do %>
     <%= label_tag :nome, 'Busca por nome' %>
     <%= text_field_tag :nome %>
     <%= submit_tag 'Buscar' %>
   <% end %>
  </div>


  <div id = "relatorioparceiro">
    <table>
      <tr>
        <th>Nome</th>
        <th>Endereço</th>
      </tr>
      

    <% @parceiros.each do |parceiro| %>
      <tr>
        <td><%= parceiro.nome %></td>
        <td><%= parceiro.endereco %></td>
      </tr>
    <% end %>
    </table>
   
   </div>
    
</div>

   <%= link_to '', consultar_relatorio_administracoes_path, :class => "voltar"%>
